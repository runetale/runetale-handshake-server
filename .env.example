# ========== STUN Server Configuration ==========
STUN_URL=                              # URL of the STUN server (e.g., stun:your-host:3478)
STUN_USERNAME=                         # STUN server username
STUN_PASSWORD=                         # STUN server password

# ========== TURN Server Configuration ==========
# For setting up your own TURN server, see: https://github.com/runetale/runetale-coturn
TURN_URL=                              # URL of the TURN server (e.g., turn:your-host:3478)
TURN_USERNAME=                         # TURN server username
TURN_PASSWORD=                         # TURN server password

# ========== Application Server Configuration ==========
PORT=                                  # The port number your application will listen on (e.g., 10000)

# ========== Logging Configuration ==========
LOG_LEVEL=                             # Logging level (e.g., debug, info, warn, error)
LOG_PATH=                              # Path to write logs to (e.g., /dev/stdout for console output)
LOG_FORMAT=                            # Format of logs: text or json

# ========== Environment Flags ==========
IS_DEV=                                # Whether the application is running in development mode (true/false)